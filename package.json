{
  "name": "padel-world-club-api",
  "version": "1.0.0",
  "description": "Modern REST API for Padel Venue Management Platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prod": " vercel --prod",
    "dev": "bun run --watch index.ts",
    "dev:fast": "NODE_ENV=development bun run index.ts",
    "bot": "bun run index.ts",
    "bot:dev": "bun run --watch index.ts",
    "bot:check": "node scripts/check-bot-token.mjs",
    "build": "bun build src/api/server.ts --outdir ./dist --target node --minify",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:full": "bun run build && bun run build:types",
    "build:vercel": "bash scripts/vercel-build.sh",
    "start": "NODE_ENV=production node dist/server.js",
    "typecheck": "bun exec tsc --project tsconfig.api.json --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run src/db/seeds/index.ts",
    "db:push": "drizzle-kit push",
    "api": "bun run src/api/server.ts",
    "api:dev": "bun run --watch src/api/server.ts",
    "api:test": "vitest run src/api/__tests__",
    "api:test:watch": "vitest src/api/__tests__",
    "tdd": "bash scripts/tdd-cycle.sh",
    "prepare": "echo 'Skipping prepare for deployment'",
    "postinstall": "node -e \"try { require('fs').symlinkSync('./example.env', './.env', 'file') } catch (e) {}\""
  },
  "keywords": [
    "padel",
    "api",
    "rest",
    "typescript",
    "express",
    "drizzle",
    "neon",
    "postgres",
    "vercel",
    "serverless"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "files": [
    "dist",
    "src",
    "example.env",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gHashTag/padle-world-club.git"
  },
  "dependencies": {
    "@telegraf/session": "^2.0.0-beta.6",
    "bcrypt": "^6.0.0",
    "chokidar": "^4.0.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.36.4",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.1.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "telegraf": "^4.16.3",
    "uuid": "^11.1.0",
    "yamljs": "^0.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.10",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/yamljs": "^0.2.34",
    "@vitest/coverage-v8": "3.1.4",
    "@vitest/ui": "3.1.4",
    "autocannon": "^8.0.0",
    "drizzle-kit": "^0.28.1",
    "eslint": "^8.56.0",
    "open": "^10.1.2",
    "supertest": "^7.1.1",
    "typescript": "^5.3.3",
    "vitest": "3.1.4",
    "tsx": "^4.7.0",
    "@types/node-fetch": "^2.6.11",
    "@types/form-data": "^2.5.0"
  }
}
